.data
	flag0: .asciiz "its sosu"
	flag1: .asciiz "its not sosu"
.text
	li $v0, 5
	syscall
	move $s0, $v0
	
	li $t0, 2
for:
	beq $t0, $s0, if
	addi $t1, $t0, 0
	div $s0, $t1	# s0/t0의 몫은 mflo에, 나머니는 mlhi에 저장됨
	mfhi $s1
	beqz $s1, else
	addi $t0, $t0, 1
	j for
	
for_end:
if:
	li $v0, 4 
	la $a0, flag0
	syscall
	
	li $v0, 10
	syscall
else:
	li $v0, 4 
	la $a0, flag1
	syscall
	
	li $v0, 10
	syscall